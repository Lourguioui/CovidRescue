{
  "expo": {
    "name": "CovideRescue",
    "slug": "CovideRescue",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "splash": {
        "mdpi": "./assets/drawable-mdpi/splash1.png",
        "hdpi": "./assets/drawable-hdpi/splash2.png",
        "xhdpi": "./assets/drawable-xhdpi/splash3.png",
        "xxhdpi": "./assets/drawable-xxhdpi/splash4.png",
        "xxxhdpi": "./assets/drawable-xxxhdpi/splash5.png",
        "resizeMode": "cover"
      },
      "package" :"com.CovidRescue.CovidRescue",
      "versionCode" : 1
      

      
    },
"ios": {
      "splash": {
        "image": "./assets/splash-ios.png",
        "resizeMode": "cover"
      },
      "supportsTablet": true
    }
    
  }
}
